@{
    Layout = null;
}
<!DOCTYPE html>
<!-- upto 2 directory depth-->
<html lang="en-US">

@*   HEAD PARTİAL VİEW ALANI *@
@await Html.PartialAsync("HeaderPartial")


<body id="top">
    @*   NavBar PARTİAL VİEW ALANI *@

    @await Html.PartialAsync("NavBarPartial")
    @*   Feature  VİEW component ALANI *@


    <div class="page-content">
        <div id="content">
            @*   Feature  VİEW component ALANI *@
            @await Component.InvokeAsync("FeatureList")

            @*   About  VİEW component ALANI *@
            @await Component.InvokeAsync("AboutList")

            @*   About  School component ALANI *@
            @await Component.InvokeAsync("SchoolList")
            <br />
            <br />

            @*   About  Experience component ALANI *@
            @await Component.InvokeAsync("ExperienceList")

            @*   About  Service component ALANI *@
            @await Component.InvokeAsync("ServiceList")

            @*   About  Skill component ALANI *@
            @await Component.InvokeAsync("SkillList")

            @*   About  Portfolio component ALANI *@
            @await Component.InvokeAsync("PortfolioList")

            <br />
            <br />
            
            


            <div class="section px-2 px-lg-4 pb-4 pt-5 mb-5" id="contact">
                <div class="container-narrow">
                    <div class="text-center mb-5">
                        <h2 class="marker marker-center">İletişim</h2>
                    </div>
                    <div class="row">
                        @*   About  SendMessage partialview ALANI *@
                        @await Html.PartialAsync("SendMessage")
                        <br />
                    </div>

                    @*   About  ContactDetails component ALANI *@
                    @await Component.InvokeAsync("ContactDetails")


                </div>
            </div>
            <footer class="pt-4 pb-4 text-center bg-light">
                <div class="container">
                    <div class="my-3">
                        <div class="h4">Semih Vural</div>
                        <p>SOFTWARE DEVELOPER | .NET / .NET CORE</p>
                        <div class="social-nav">
                            <nav role="navigation">
                                <ul class="nav justify-content-center">

                                    <li class="nav-item"><a class="nav-link" href="https://www.linkedin.com/in/semih-vural/" title="LinkedIn" target="_blank"><i class="fab fa-linkedin"></i><span class="menu-title sr-only">LinkedIn</span></a></li>
                                    <li class="nav-item"><a class="nav-link" href="https://github.com/SemihVRL" title="GitHub" target="_blank"><i class="fab fa-github"></i><span class="menu-title sr-only">GitHub</span></a></li>
                                  @*   <li class="nav-item"><a class="nav-link" href="https://www.instagram.com/semiihvurrall/" title="Instagram" target="_blank"><i class="fab fa-instagram"></i><span class="menu-title sr-only">Instagram</span></a></li>
                                </ul> *@
                            </nav>
                        </div>
                    </div>
                    <div class="text-small text-secondary">
                        <div class="mb-1">&copy;@DateTime.Now.Year Semih Vural Tarafından Hazırlanmıştır.</div>

                    </div>
                </div>
            </footer>
        </div>
    </div>
    <div id="scrolltop"><a class="btn btn-secondary" href="#top"><span class="icon"><i class="fas fa-angle-up fa-x"></i></span></a></div>
    <script src="~/Template/scripts/imagesloaded.pkgd.min.js?ver=1.2.0"></script>
    <script src="~/Template/scripts/masonry.pkgd.min.js?ver=1.2.0"></script>
    <script src="~/Template/scripts/BigPicture.min.js?ver=1.2.0"></script>
    <script src="~/Template/scripts/purecounter.min.js?ver=1.2.0"></script>
    <script src="~/Template/scripts/bootstrap.bundle.min.js?ver=1.2.0"></script>
    <script src="~/Template/scripts/aos.min.js?ver=1.2.0"></script>
    <script src="~/Template/scripts/main.js?ver=1.2.0"></script>
</body>
</html>
